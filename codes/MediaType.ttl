@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix ids: <https://w3id.org/ids/core/> .
@prefix idsm: <https://w3id.org/ids/metamodel/> .
@prefix idsc_media: <https://w3id.org/ids/code/mediatype/> .

<https://w3id.org/ids/code/mediatype/>
    a owl:Ontology ;
    dct:title "IDS Media Type Vocabulary" ;
    rdfs:comment "Collection of Media Types instances used by the International Data Space."@en ;
    dct:date "2018-10-01" ;
    owl:versionInfo "1.0.0" ;
    .

# Instances
# ---------

# Content

idsc_media:TEXT_PLAIN
    a ids:IANAMediaType ;
	rdfs:label "text/plain" ;
	rdfs:comment "Plain text file" ;
	owl:sameAs <www.sparontologies.net/mediatype/text/plain> ;
	ids:filenameExtension "txt" ;
	.

idsc_media:TEXT_CSV
    a ids:IANAMediaType ;
	rdfs:label "text/csv" ;
	rdfs:comment "Comma-separated text file (CSV)" ;
	owl:sameAs <www.sparontologies.net/mediatype/text/csv> ;
	ids:filenameExtension "json" ;
	.

idsc_media:TEXT_XML
    a ids:IANAMediaType ;
	rdfs:label "text/xml" ;
	rdfs:comment "Alias for the application/xml Media Type."@en ;
	owl:sameAs <www.sparontologies.net/mediatype/text/xml> ;
	ids:filenameExtension "xml" ;
	.

idsc_media:APPLICATION_JSON
    a ids:IANAMediaType ;
	rdfs:label "application/json" ;
	rdfs:comment "JavaScript Object Notation file"@en ;
	# TODO: Use local resource name instead
	# idsm:constantIdentifier "APPLICATION_JSON" ;
	owl:sameAs <www.sparontologies.net/mediatype/application/json> ;
	ids:filenameExtension "json" ;
	.

# Container

idsc_media:APPLICATION_ZIP
    a ids:IANAMediaType ;
    owl:sameAs <www.sparontologies.net/mediatype/application/zip> ;
	rdfs:label "application/zip" ;
	idsm:constantIdentifier "APPLICATION_ZIP" ;
	rdfs:isDefinedBy <https://www.iana.org/assignments/media-types/application/zip> ;
	ids:magicNumber "50 4B 03 04" ;
	ids:magicNumber "50 4B 05 06" ;
	ids:magicNumber "50 4B 07 08" ;
	ids:filenameExtension "zip" ;
	.
