@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix ids: <https://w3id.org/ids/core/> .
@prefix idsc_qpt: <https://w3id.org/ids/code/qpt/> .
@prefix idsm: <https://w3id.org/ids/metamodel/> .

# Instances
# ---------

idsc_qpt:IDENTIFIER a ids:QueryPayloadType;
	rdfs:label "IDENTIFIER";
	rdfs:comment "Query payload holds an identifier string.";
	idsm:constantIdentifier "IDENTIFIER".

idsc_qpt:QUERYSTRING a ids:QueryPayloadType;
	rdfs:label "QUERYSTRING";
	rdfs:comment "Query payload holds a query string (e.g., SPARQL, SQL)";
	idsm:constantIdentifier "QUERYSTRING".

