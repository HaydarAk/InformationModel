@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix ids: <https://w3id.org/ids/core/> .
@prefix idsc_unit: <https://w3id.org/ids/code/unit/> .
@prefix idsm: <https://w3id.org/ids/metamodel/> .

# Instances
# ---------

idsc_unit:euro a ids:Unit;
    rdfs:label "Euro";
    idsm:constantIdentifier "EUR".

idsc_unit:usd a ids:Unit;
    rdfs:label "US Dollar";
    idsm:constantIdentifier "USD".

idsc_unit:kb a ids:Unit;
    rdfs:label "Kilobyte";
    idsm:constantIdentifier "KB".

idsc_unit:day a ids:Unit;
    rdfs:label "Day";
    idsm:constantIdentifier "DAY".

idsc_unit:week a ids:Unit;
    rdfs:label "Week";
    idsm:constantIdentifier "WEEK".

idsc_unit:month a ids:Unit;
    rdfs:label "Month";
    idsm:constantIdentifier "MONTH".

idsc_unit:year a ids:Unit;
    rdfs:label "Year";
    idsm:constantIdentifier "YEAR".
