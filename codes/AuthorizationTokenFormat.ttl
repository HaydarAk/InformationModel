@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix ids: <https://w3id.org/ids/core/> .
@prefix idsc_atf: <https://w3id.org/ids/code/authtokenformat/> .
@prefix idsm: <https://w3id.org/ids/metamodel/> .

# Instances
# ---------

idsc_atf:jwt a ids:AuthorizationTokenFormat ;
    rdfs:label "Json Web Token";
	idsm:constantIdentifier "JWT".

idsc_atf:swt a ids:AuthorizationTokenFormat ;
    rdfs:label "Simple Web Token";
	idsm:constantIdentifier "SWT".

idsc_atf:saml11 a ids:AuthorizationTokenFormat ;
    rdfs:label "Security Assertion Markup Language (SAML) 1.1";
	idsm:constantIdentifier "SAML11".

idsc_atf:saml20 a ids:AuthorizationTokenFormat ;
    rdfs:label "Security Assertion Markup Language (SAML) 2.0";
    idsm:constantIdentifier "SAML20".


