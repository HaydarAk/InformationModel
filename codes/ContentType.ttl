@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix ids: <https://w3id.org/ids/core/> .
@prefix idsm: <https://w3id.org/ids/metamodel/> .
@prefix idsc_content: <https://w3id.org/ids/code/content/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

# Instances
# ---------

idsc_content:opaque a ids:ContentType;
    rdfs:label "Opaque";
	rdfs:label "Opaque, not further specified type of (binary) content. ";
	idsm:constantIdentifier "opaque" .

idsc_content:data a ids:ContentType;
    rdfs:label "Data";
	rdfs:label "Custom or generic (structured) data, i.e. arrangement of data fields (optionally defined by a schema).";
	idsm:constantIdentifier "data" .
    
idsc_content:document a ids:ContentType;
    rdfs:label "Document";
	rdfs:label "Informative textual content intended for reading and interpretation by humans and other intelligent agents. ";
	idsm:constantIdentifier "document" ;
    skos:note "This content type generlizes the notion of Media Type 'text' as a meaningful sequence of characters written in a set of human language(s), regerdless of the formating and file type (e.g. plain, Word or PostScript)." ;
    rdfs:seeAlso <https://tools.ietf.org/html/rfc2046#section-4.1> .

idsc_content:software  a ids:ContentType;
    rdfs:label "Software";
	rdfs:label "Executable software (binary), interpreted program code (source) or fragments thereof. Intended purpose is an optional preprocessing (i.e. compilation, installation) and subsequnt execution/invokation of the software.";
	idsm:constantIdentifier "software" ;
    skos:note "Term 'application' is misleading, since Mime Type of same name refers to unspecified data processed by some type of application program." ;
    rdfs:seeAlso <https://tools.ietf.org/html/rfc2046#section-4.5> .

idsc_content:audio  a ids:ContentType;
    rdfs:label "Audio";
	rdfs:label "Content intended for auditive perception, requires an audio output device (i.e. a loudspeaker) to present the contents.";
	idsm:constantIdentifier "audio" ;
    rdfs:seeAlso <https://tools.ietf.org/html/rfc2046#section-4.3> .

idsc_content:image a ids:ContentType;
    rdfs:label "Image";
	rdfs:label "Static content intended for visual perception, requires a display device (i.e. a screen) to present the contents.";
	idsm:constantIdentifier "image" ;
    rdfs:seeAlso <https://tools.ietf.org/html/rfc2046#section-4.2> .

idsc_content:video a ids:ContentType;
    rdfs:label "Video";
	rdfs:label "Dynmic content intended for visual and auditive perception, combines requirements of image and audio Content Types, while imposing further requirements on video decoding etc.";
	idsm:constantIdentifier "video" ;
    rdfs:seeAlso <https://tools.ietf.org/html/rfc2046#section-4.4> .

idsc_content:multipart a ids:ContentType;
    rdfs:label "Multipart";
	rdfs:label "Container entity composed of multiple (individually defined) parts.";
	idsm:constantIdentifier "multipart" ;
    rdfs:seeAlso <https://tools.ietf.org/html/rfc2046#section-5.1> .
