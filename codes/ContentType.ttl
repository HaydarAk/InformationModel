@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix ids: <https://w3id.org/ids/core/> .
@prefix idsm: <https://w3id.org/ids/metamodel/> .
@prefix idsc_content: <https://w3id.org/ids/code/content/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

# Instances
# ---------

idsc_content:opaque a ids:ContentType;
    rdfs:label "Opaque";
	rdfs:comment "Opaque, not further specified type of (binary) content. ";
	dct:identifier "opaque" .

idsc_content:data a ids:ContentType;
    rdfs:label "Data";
	rdfs:comment "Custom or generic (structured) data, i.e. arrangement of data fields (optionally defined by a schema).";
	dct:identifier "data" .
    
idsc_content:document a ids:ContentType;
    rdfs:label "Document";
	rdfs:comment "Informative textual content intended for reading and interpretation by humans and other intelligent agents. ";
	dct:identifier "document" ;
    skos:note "This content type generlizes the notion of Media Type 'text' as a meaningful sequence of characters written in a set of human language(s), regerdless of the formating and file type (e.g. plain, Word or PostScript)." ;
    rdfs:seeAlso <https://tools.ietf.org/html/rfc2046#section-4.1> .

idsc_content:software  a ids:ContentType;
    rdfs:label "Software";
	rdfs:comment "Executable software (binary), interpreted program code (source) or fragments thereof. Intended purpose is an optional preprocessing (i.e. compilation, installation) and subsequnt execution/invokation of the software.";
	dct:identifier "software" ;
    skos:note "Term 'application' is misleading, since Mime Type of same name refers to unspecified data processed by some type of application program." ;
    rdfs:seeAlso <https://tools.ietf.org/html/rfc2046#section-4.5> .

idsc_content:audio  a ids:ContentType;
    rdfs:label "Audio";
	rdfs:comment "Content intended for auditive perception, requires an audio output device (i.e. a loudspeaker) to present the contents.";
	dct:identifier "audio" ;
    rdfs:seeAlso <https://tools.ietf.org/html/rfc2046#section-4.3> .

idsc_content:image a ids:ContentType;
    rdfs:label "Image";
	rdfs:comment "Static content intended for visual perception, requires a display device (i.e. a screen) to present the contents.";
	dct:identifier "image" ;
    rdfs:seeAlso <https://tools.ietf.org/html/rfc2046#section-4.2> .

idsc_content:video a ids:ContentType;
    rdfs:label "Video";
	rdfs:comment "Dynmic content intended for visual and auditive perception, combines requirements of image and audio Content Types, while imposing further requirements on video decoding etc.";
	dct:identifier "video" ;
    rdfs:seeAlso <https://tools.ietf.org/html/rfc2046#section-4.4> .

idsc_content:multipart a ids:ContentType;
    rdfs:label "Multipart";
	rdfs:comment "Container entity composed of multiple (individually defined) parts.";
	dct:identifier "multipart" ;
    rdfs:seeAlso <https://tools.ietf.org/html/rfc2046#section-5.1> .
