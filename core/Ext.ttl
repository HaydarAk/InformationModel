@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

@prefix ids: <https://schema.industrialdataspace.org/core/> .
@prefix ids_lc: <https://schema.industrialdataspace.org/extcore/license/> .
@prefix ids_rel: <https://schema.industrialdataspace.org/helper/relation/> .
@prefix ids_trans: <https://schema.industrialdataspace.org/helper/transformation/> .

# Classes
# -------

ids:Industry a owl:Class;
    trns:isAbstract true;
    rdfs:label "Industry"@en ;
    rdfs:comment "The class of industries that can be distinguished by their activities.".

ids:LicenseDocument rdfs:subClassOf dct:LicenseDocument;
    a owl:Class;
    rdfs:label "LicenseDocument"@en ;
    rdfs:comment "The class of license documents to be attached to DataAssets.".

ids:Unit a owl:Class;
    rdfs:label "Unit"@en ;
    rdfs:comment "The class of units.".

