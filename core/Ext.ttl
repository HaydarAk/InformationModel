@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

@prefix ids: <http://industrialdataspace.org/2016/10/ids/core#> .
@prefix ids_lc: <http://industrialdataspace.org/2016/10/ids/ext/license#> .
@prefix valid: <http://industrialdataspace.org/2017/01/ids/validation#> .
@prefix trns: <http://industrialdataspace.org/2017/01/ids/transformation#> .

# Classes
# -------

ids:Industry a owl:Class;
    trns:isAbstract true;
    rdfs:label "Industry"@en ;
    rdfs:comment "The class of industries that can be distinguished by their activities.".

ids:LicenseDocument rdfs:subClassOf dct:LicenseDocument;
    a owl:Class;
    rdfs:label "LicenseDocument"@en ;
    rdfs:comment "The class of license documents to be attached to DataAssets.".

ids:Unit a owl:Class;
    rdfs:label "Unit"@en ;
    rdfs:comment "The class of units.".

