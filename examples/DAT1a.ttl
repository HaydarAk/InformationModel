# Modeling approach: enumerated content descriptions / instantiations

# TODO -> make part of the connector
:HTTP_HOST
    a ids:Host ;
    ids:protocol idsc_prot:HTTP2;
    ids:accessUrl "https://myserver.org"^^xsd:anyURI .

:DAT1
    ids:title "European highway statistics"@en ;
    ids:description "Data collection about usage statistics of European highways"@en;
    ids:keyword "highway", "statistics", "Europe";
    # Compressed representation of the overall resource
    ids:representation [
        a ids:ContainerRepresentation ;
        ids:packaging idsc_pack:TAR ;
        ids:compression idsc_pack:GZIP ;
        ids:instance :DAT1_TAR_GZIP 
    ] ;
    ids:contentPart :E1_01_2000 ;
    #
    # many enumerated data sets ..
    #
    ids:contentPart :E421_09_2018 ;
    # 
    ids:endpoint [
        ids:endpointHost :HTTP_HOST ;
        ids:endpointContent :DAT1_TAR_GZIP ;
        ids:path "/Archive" 
    ] ;
    ids:endpoint [
        ids:endpointHost :HTTP_HOST ;
        ids:endpointContent :E1_01_2000 ;
        ids:path "/content/E1_01_2000.csv"
    ] .
    

# Resources archive available download
:DAT1_TAR_GZIP
    a ids:ContentInstance ;
    ids:byteSize 2123178072 ;
    ids:fileName "EuropeanHighwayStatistics_2000_2018.tar.gzip" .

# Enumeration of individual content instances
:E1_01_2000
    a ids:Data ;
    ids:spatialCoverage [] ;
    ids:temporalCoverage [] ; # 01 2000
    ids:representation [
        a ids:Representation ;
        ids:mediaType idsc_media:TEXT_CSV ;
        ids:instance :E1_01_2000_CSV 
    ] .

:E1_01_2018_CSV
    a ids:ContentInstance;
    ids:byteSize 9823132;
    ids:fileName "E1_01_2000.csv" .

:E421_09_2018
    a ids:Data ;
    ids:spatialCoverage [] ;
    ids:temporalCoverage [] ; # 09 2018
    ids:representation [
        a ids:Representation ;
        ids:mediaType idsc_media:TEXT_CSV ;
        ids:instance :E421_09_2018 
    ] .

:E421_09_2018
    a ids:ContentInstance;
    ids:byteSize 6174035;
    ids:fileName "E421_09_2018.csv" .
    
