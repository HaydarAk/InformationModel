@prefix ids: <https://w3id.org/idsa/core/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix idsc_lang: <https://w3id.org/idsa/code/language/> .
@prefix idsc_media: <https://w3id.org/idsa/code/mediatype/> .
@prefix idsc_action: <https://w3id.org/idsa/code/action/> .
@prefix idsc_lic: <https://w3id.org/idsa/code/license/> .
@prefix idsc_unit: <https://w3id.org/idsa/code/unit/> .
@prefix idsc_prot: <https://w3id.org/idsa/code/Protocol/> .
@prefix idsc_auth: <https://w3id.org/idsa/code/Auth/> .
@prefix idsc_sec: <https://w3id.org/idsa/code/security_guarantee/> .
@prefix idsc_cert: <https://w3id.org/idsa/code/certification/> .

@prefix conn1: <https://aastat.gov.de/connector/conn1/> .
@prefix sample_Data: <https://aastat.gov.de/connector/conn1/sample_Data/> .


# Example of a Trusted Connector in the Information Model.
conn1:
    a ids:TrustedConnector ;
    # General properties
    ids:title "EU Traffic statistics hub" ;
    ids:description "Example Trusted Connector maintained by <IDS Participant>" ;
    ids:maintainer "<some IDS participant>";
    ids:curator "<some IDS participant>";
    ids:physicalLocation <http://sws.geonames.org/3247450/> ;
	ids:outboundModelVersion "1.0.3" ;
	
	# Information Model version that the Connector is capable of reading/processing
	ids:inboundModelVersion "1.0.3" ;
	
    # Network host(s) maintained by the Connector. Describes the protocol and access URL of the host.
    ids:host [
		a ids:Host ;
		ids:protocol idsc_prot:HTTP2;
		ids:accessUrl "https://connector.aastat.gov.de"^^xsd:anyURI ;
		];
	
	# Default host of the Connector. May be used for basic infrastructure interactions, e.g., providing the self description
	ids:defaultHost [
		a ids:Host ;
		ids:protocol idsc_prot:HTTP2;
		ids:accessUrl "https://connector.aastat.gov.de"^^xsd:anyURI ;
		];

    # Security and trust features
	# Holds information regarding the authentication so that connections to the Connector's data offerings can be establised.
	# Properties contain the authentication service URI and authentication protocol (OAUTH2_JWT)
    ids:authInfo [
		a ids:AuthInfo ;
		ids:authService "https://keycloak.aastat.gov.de"^^xsd:anyURI ;
		ids:authStandard idsc_auth:OAUTH2_JWT ;
		];
	
    # Information on the security profile of the connector
    ids:securityProfile idsc_sec:TRUSTED_CONNECTOR_SECURITY_PROFILE;
    
	# Information regarding the certification of the Connector including the certification level and validity date.
	ids:componentCertification [
		a ids:ComponentCertification ;
	    ids:certificationLevel idsc_cert:COMPONENT_TRUSTPLUS_SECURITY_PROFILE_CONCEPT_REVIEW ;
		ids:lastValidDate "2020-12-31"^^xsd:date ;
	];

    # Catalog of offered and requested resources
    ids:catalog [
        a ids:Catalog;
        ids:offer "reference to resources offered by the the connector" ;
		ids:request "reference to the resources requested by the connector";
    ].