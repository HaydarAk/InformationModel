@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix dcmitype: <http://purl.org/dc/dcmitype/> .
@prefix ids: <https://w3id.org/ids/core/> .
@prefix idsm: <https://w3id.org/ids/metamodel/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix idsc_media: <https://w3id.org/ids/code/mediatype/> .
@prefix exs: <urn:example:server:pattern:> .
@prefix idsc_pack: <https://w3id.org/ids/code/pack/> .
@prefix idsc_lic: <https://w3id.org/ids/code/license/> .

exs:RS1
    a ids:SimpleResource; # there is only a single content provided
    ids:title "Holiday photo archive 2018"@en ;
    ids:description "Pictures of the great summer break 2018"@en;
    ids:keyword "summer", "holiday", "picture";
    ids:contentPart exs:RS1_CNT;
    ids:licence idsc_lic:OtherOpenLicense .

exs:RS1_CNT # Top-level contents of a resource must not be bnodes!
    a ids:Container;
    # contents below are embedded, do not show up at resource level,
    # they might become indexed and improve recall of the resource
    ids:contentPart [
        # incomplete content description is fine, when sufficient
        a ids:Image ;
        ids:title "Sunset at the beach";
    ],
    [
        a ids:Image ;
        ids:title "Scary black licorice ice cream";
        ids:representation [
            a ids:ImageRepresentation ;# typed representation to attach type-specific properties
            ids:mediaType idsc_media:image_jpeg; # image/jpeg is missing in codes?
            # dimensions are often shared by pictures of a series, they are less individual than e.g. "size", attached to representation
            ids:width 5184; 
            ids:height 3888;
            # these are individual properties of the file
            ids:artifact [
                ids:byteSize 6174035;
                ids:fileName "DSC00597.jpeg"
            ]
        ]
    ];
    ids:representation exs:RS1_REP.
    
exs:RS1_REP
    a ids:ContainerRepresentation ;
    ids:packaging idsc_pack:TAR;
    ids:compression idsc_pack:GZIP;
    ids:artifact exs:RS1_ART.

exs:RS1_ART
    a ids:Artifact;
    ids:byteSize 123178072;
    ids:fileName "HolidayPictures2018.tar.gzip" .
        
_:e
    # the endpoint complements the artifact by the "communication" aspect, e.g. an URL
    a ids:HttpEndpoint;
    ids:resourceInstance exs:RS1_ART;
    ids:url <http://filecloud.org/me/pictures/archive/holiday2018> .

