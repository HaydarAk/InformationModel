@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix ids: <https://w3id.org/idsa/core/> .
@prefix idsc_concept: <https://w3id.org/idsa/code/concept/> .
@prefix idsm: <https://w3id.org/idsa/metamodel/> .

# Instances
# ---------

idsc_concept:concept
    a skos:ConceptScheme ;
    dct:title "Collection of concepts referenced by content" .

idsc_concept:place
    a skos:Concept ;
    skos:topConceptOf idsc_concept:concept;
    skos:prefLabel "place"@en .

idsc_concept:time
    a skos:Concept ;
    skos:topConceptOf idsc_concept:concept;
    skos:prefLabel "time"@en .

idsc_concept:HIGHWAY
    a skos:Concept ;
    skos:broader idsc_concept:place;
    skos:prefLabel "highway"@en .

idsc_concept:YEAR
    a skos:Concept ;
    skos:broader idsc_concept:time;
    skos:prefLabel "year"@en .

idsc_concept:MONTH
    a skos:Concept ;
    skos:broader idsc_concept:time;
    skos:prefLabel "month"@en .
