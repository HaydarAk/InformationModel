# Abstract data types
# -------------------

ids:Opaque a owl:Class;
#    rdfs:subClassOf ids:ContentType ;
    owl:disjointWith ids:Structure ;
    rdfs:label "Opaque"@en ;
    rdfs:comment "Opaque type of Resource that is not further specified."@en .
    


ids:Application a owl:Class ;
#    rdfs:subClassOf ids:ContentType ;
    rdfs:label "Application"@en ;
    rdfs:comment "IDS Application."@en .

ids:SortOrder a owl:Class ;
    rdfs:label "Sort order"@en ;    
    rdfs:comment "Sort order of entries within an ordered Collection."@en .
    
# Properties
# ----------

ids:ordered
    a owl:DatatypeProperty ;
    rdfs:label "ordered"@en;
    rdfs:domain ids:ContentType ;
    rdfs:range xsd:boolean ;
    rdfs:comment "Property indicating whether this type of Content supports ordering in a particular way (natural and user-driven ordering)."@en .

# Renamed from orderPolicy
ids:sortOrder a owl:ObjectProperty;
    rdfs:label "sort order"@en;
    rdfs:domain ids:Ordered;
    rdfs:range ids:SortOrder;
    rdfs:comment "Indicates the sort order of an ordered Collection."@en.

