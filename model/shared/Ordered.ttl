@prefix dct: <http://purl.org/dc/terms/> .
@prefix dcmitype: <http://purl.org/dc/dcmitype/> .
@prefix ids: <https://w3id.org/ids/core/> .
@prefix idsm: <https://w3id.org/ids/metamodel/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .


ids:Ordered a owl:Class;
#    rdfs:subClassOf ids:ContentType ;
    owl:equivalentClass
    [
      rdf:type          owl:Restriction ;
      owl:onProperty    ids:ordered ;
      owl:hasValue      "true"^^xsd:boolean 
    ];
    rdfs:label "Ordered content"@en ;
    rdfs:comment "Type of Digital Content ordered in a particular way. Serves as rdfs:domain of ordering-related properties."@en .

