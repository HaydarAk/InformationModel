# Classes
# -------

ids:Communicable
    rdf:type owl:Class ;
    idsm:abstract true;
    rdfs:label "Communicable"@en ;    
    rdfs:comment "Data that is capable of being (digitally) communicated via a Data Endpoint."@en .

# Properties
# ----------

ids:endpoint
    a owl:ObjectProperty ;    
    rdfs:domain ids:Communicable ;
    rdfs:range ids:Endpoint ;
    rdfs:label "endpoint"@en ;
    rdfs:comment "Reference to the mediating Endpoint."@en.
