@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix ids: <https://w3id.org/ids/core/> .
@prefix idsm: <https://w3id.org/ids/metamodel/> .

# Classes
# -------

ids:ConnectionOption a owl:Class;
    rdfs:label "ConnectionOption"@en ;
    rdfs:comment "Class that aggregates Protocol types and URLs and Paths for using them for InfrastructureComponent interactions."@en.

# Properties
# ----------

ids:protocol a owl:ObjectProperty;
    rdfs:domain ids:ConnectionOption;
    rdfs:range ids:Protocol ;
    rdfs:label "httpBaseUrl"@en;
    rdfs:comment "References the supported communication protocol."@en.

ids:accessUrl a owl:DatatypeProperty;
    rdfs:domain ids:Connectivity;
    rdfs:range xsd:anyURI ;
    rdfs:label "Access URL"@en;
    rdfs:comment "Defines the URL (e.g., websocket host and path, mqtt broker ip address and port) required for communication using the protocol."@en.