@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix org: <http://www.w3.org/ns/org#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix ids: <https://w3id.org/ids/core/> .
@prefix idsm: <https://w3id.org/ids/metamodel/> .

# Classes
# -------

idsm:ModelView
    a owl:Class ;
	rdfs:label "ModelView"@en ;
    rdfs:comment "Selective view defined upon a model."@en .

# Properties
# ----------

idsm:view a owl:ObjectProperty ;
    rdfs:domain rdf:Resource ;
    rdfs:range idsm:ModelView ;
    rdfs:label "view"@en ;
    rdfs:comment "Model view associated with given resource."@en.

