@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix ids: <https://w3id.org/idsa/core/> .
@prefix idsm: <https://w3id.org/idsa/metamodel/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .

<#ParameterShape>
	a sh:NodeShape ; 
	sh:targetClass ids:Parameter ;

	sh:property [
		a sh:PropertyShape ;
		sh:path ids:parameterContent ;
		sh:class ids:DigitalContent ;
		sh:minCount 1 ;
		sh:severity sh:Violation ;
		sh:message "An ids:Parameter must have at least one ids:DigitalContent linked through the ids:parameterContent property"@en ; 
	] .

