@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ids: <https://w3id.org/idsa/core/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .

<#ParticipantCertificationShape>
	a sh:NodeShape ; 
	sh:targetClass ids:ParticipantCertification ;

	sh:property [
		a sh:PropertyShape ;
		sh:path ids:membershipEnd ;
		sh:datatype xsd:date ;
		sh:severity sh:Violation ;
		sh:message "An ids:membershipEnd property must point from an ids:ParticipantCertification to an xsd:date."@en ; 
	] .

