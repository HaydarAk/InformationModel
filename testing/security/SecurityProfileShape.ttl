@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix ids: <https://w3id.org/idsa/core/> .
@prefix idsm: <https://w3id.org/idsa/metamodel/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .

<#CustomSecurityProfileShape>
	a sh:NodeShape ; 
	sh:targetClass ids:CustomSecurityProfile ;

	sh:property [
		a sh:PropertyShape ;
		sh:path ids:securityGuarantee ;
		sh:class ids:SecurityGuarantee ;
		sh:severity sh:Violation ;
		sh:message "An ids:securityGuarantee property must point from an ids:CustomSecurityProfile to an ids:SecurityGuarantee."@en ; 
	] .

