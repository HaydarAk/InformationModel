@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dct: <http://purl.org/dc/terms/> .

@prefix idoc: <http://industrialdataspace.org/2017/01/ids/documentation#> .

# Description of this ontology
# ----------------------------

idoc: a owl:Ontology;
    dct:title "The Industrial Dataspace Information Model Documentation Ontology"@en;
    rdfs:comment "An ontology that defines classes and properties for additional documentation of the Information Model which are needed to generate the Information Model API.";
    dct:creator "Christian Mader" ;
    dct:creator "Jaroslav Pullmann" ;
    dct:creator "Niklas Petersen" ;
    dct:creator "Christoph Lange-Bever" ;
    dct:creator "Steffen Lohmann" ;
    dct:publisher "Fraunhofer IAIS/EIS";
    dct:publisher "Fraunhofer FIT";
    dct:date "2017-10-18";
    owl:versionInfo "0.0.1".

# Classes
# -------

# Properties
# ----------

idoc:labelPluralForm a owl:DatatypeProperty;
    rdfs:subPropertyOf skos:prefLabel;
	rdfs:label "labelPluralForm"@en;
	rdfs:domain rdfs:Resource;
	rdfs:range rdf:PlainLiteral;
	rdfs:comment "The preferred lexical label for a resource, in plural form in a given language."@en.
