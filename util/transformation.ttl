@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

@prefix trns: <http://industrialdataspace.org/2017/01/ids/transformation#> .

# Description of this ontology
# ----------------------------

trns: a owl:Ontology;
    dct:title "The Industrial Dataspace Information Model Transformation Ontology"@en;
    rdfs:comment "An ontology to enrich Information Model classes and properties with information relevant for proper generation of an API for the Information Model.";
    dct:creator "Christian Mader" ;
    dct:creator "Jaroslav Pullmann" ;
    dct:creator "Niklas Petersen" ;
    dct:creator "Christoph Lange-Bever" ;
    dct:creator "Steffen Lohmann" ;
    dct:publisher "Fraunhofer IAIS/EIS";
    dct:publisher "Fraunhofer FIT";
    dct:date "2017-10-18";
    owl:versionInfo "0.0.1".

# Classes
# -------

# Properties
# ----------

trns:isAbstract a owl:DatatypeProperty;
    rdfs:label "isAbstract"@en;
    rdfs:domain owl:Class;
    rdfs:range xsd:boolean;
    rdfs:comment "Indicates that the generated API class should be abstract."@en.

trns:refByURI a owl:DatatypeProperty;
    rdfs:label "refByURI"@en;
    rdfs:domain owl:ObjectProperty;
    rdfs:range xsd:boolean;
    rdfs:comment "Indicates that the generated method that represents the ObjectProperty in the API should reference the domain as URI instead of the actual class name."@en.