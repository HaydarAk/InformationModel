# Classes
# -------

ids:ResourceIdentifier
    rdf:type owl:Class ;
    rdfs:subClassOf ids:Identifier ;
    rdfs:label "Resource identifier"@en ;
    rdfs:comment "Globally unique Identifier of an IDS Resource."@en .

# Example of a derived datatype, supplies an interpretation for anyURI
#
#ids:ResourceIdentifier owl:equivalentClass
#  [ rdf:type  rdfs:Datatype;
#    owl:onDatatype  xsd:anyURI;
#    owl:withRestrictions (
#       [ xsd:minLength  "1"^^xsd:integer ]
#       [ xsd:pattern ".*/resource/.*" ]
#    )
#  ] .
# Usage: "https://server.org/resource/123"^^ids:ResourceIdentifier
# Pros: allows for definition of identifier syntax rules, is more specific about interpretation type than anyURI
# Cons: more verbose ..(?)



# ids:ParticipantIdentifier, attributed to Agent, since parties are not necessarily IDS Participants:

ids:AgentIdentifier
    rdf:type owl:Class ;
    rdfs:subClassOf ids:Identifier ;
    rdfs:label "Agent identifier"@en ;
    rdfs:comment "Unique Identifier of an Agent as a party involved in Resource exchange."@en .

ids:ConnectorIdentifier
    rdf:type owl:Class ;
    rdfs:subClassOf ids:Identifier ;
    rdfs:label "Connector identifier"@en ;
    rdfs:comment "Globally unique Identifier of an IDS Connector."@en .        

ids:MessageIdentifier
    rdf:type owl:Class ;
    rdfs:subClassOf ids:Identifier ;
    rdfs:label "Message identifier"@en ;
    rdfs:comment "Unique Identifier of an IDS Message."@en .        
