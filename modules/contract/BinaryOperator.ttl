@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .

@prefix ids_contr: <https://schema.industrialdataspace.org/contract/> .

# Classes
# -------

ids_contr:BinaryOperator rdfs:subClassOf odrl:Operator;
    a owl:Class;
    rdfs:label "BinaryOperator"@en;
    rdfs:comment "The class of binary operators."@en.

# Instances
# ---------

ids_contr:eq a ids_contr:BinaryOperator;
    rdfs:seeAlso odrl:eq;
    rdfs:comment "Equals operator.";
    dct:identifier "EQUALS".

ids_contr:gt a ids_contr:BinaryOperator;
    rdfs:seeAlso odrl:gt;
    rdfs:comment "Greater-than operator.";
    dct:identifier "GREATER_THAN".

ids_contr:lt a ids_contr:BinaryOperator;
    rdfs:seeAlso odrl:lt;
    rdfs:comment "Less-than operator.";
    dct:identifier "LESS_THAN".