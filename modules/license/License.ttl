@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

@prefix ids_lc: <https://schema.industrialdataspace.org/license/> .

# Classes
# -------

ids_lc:LicenseDocument rdfs:subClassOf dct:LicenseDocument;
    a owl:Class;
    rdfs:label "LicenseDocument"@en ;
    rdfs:comment "The class of license documents to be attached to DataAssets.".

# Instances
# ---------

ids_lc:license_cc_by_nc_nd_2_0 a ids_lc:LicenseDocument;
    rdfs:seeAlso <https://creativecommons.org/licenses/by-nc-nd/2.0/>;
    rdfs:comment "Attribution-NonCommercial-NoDerivs 2.0 Generic (CC BY-NC-ND 2.0)";
    dct:identifier "CC_BY_NC_ND_2_0".

ids_lc:officialWork a ids_lc:LicenseDocument;
    rdfs:seeAlso <http://www.gesetze-im-internet.de/urhg/__5.html>;
    rdfs:comment "Amtliches Werk, lizenzfrei nach §5 Abs. 1 UrhG";
    dct:identifier "OFFICIAL_WORK".

