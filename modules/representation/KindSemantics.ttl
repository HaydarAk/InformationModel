@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dct: <http://purl.org/dc/terms/> .

@prefix ids_rep: <https://schema.industrialdataspace.org/representation/> .
@prefix ids_trans: <https://schema.iais.fraunhofer.de/ids/helper/transformation/> .

# Classes
# -------

ids_rep:KindSemantics a owl:Class;
    ids_trans:isAbstract true;
    rdfs:label "KindSemantics"@en ;
    rdfs:comment "The class of defined high-level semantics for DataAsset Kindsorder policies of Collections.".

ids_rep:CoreKindSemantics rdfs:subClassOf ids_rep:KindSemantics;
    a owl:Class;
    rdfs:label "CoreKindSemantics"@en ;
    rdfs:comment "The class of KindSemantics defined in the core information model.".

# Instances
# ---------

ids_rep:hash a ids_rep:CoreKindSemantics;
    rdfs:comment "hash";
    dct:identifier "HASH".

