    rdfs:comment "date";
    dct:identifier "DATE".

ids_rep:textual a ids_rep:CoreKindSemantics;
    rdfs:comment "textual";
    dct:identifier "TEXTUAL".

ids_rep:binary a ids_rep:CoreKindSemantics;
    rdfs:comment "binary";
    dct:identifier "BINARY".

ids_rep:image a ids_rep:CoreKindSemantics;
    rdfs:comment "image";
    dct:identifier "IMAGE".

ids_rep:identifier a ids_rep:CoreKindSemantics;
    rdfs:comment "identifier";
    dct:identifier "IDENTIFIER".

ids_rep:sensor a ids_rep:CoreKindSemantics;
    rdfs:comment "sensor measurement";
    dct:identifier "SENSOR_MEASUREMENT".

