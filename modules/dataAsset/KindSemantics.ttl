@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix ids: <https://w3id.org/ids/core> .

# Classes
# -------

ids:KindSemantics a owl:Class;
    ids:isAbstract true;
    rdfs:label "KindSemantics"@en ;
    rdfs:comment "The class of defined high-level semantics for DataAsset Kinds.".

# Instances
# ---------

ids:hash a ids:KindSemantics;
    rdfs:comment "hash";
    dct:identifier "HASH".

ids:date a ids:KindSemantics;
    rdfs:comment "date";
    dct:identifier "DATE";
    skos:narrower ids:startDate;
    skos:narrower ids:endDate.

ids:startDate a ids:KindSemantics;
    rdfs:comment "start date";
    dct:identifier "START_DATE";
    skos:broader ids:date.

ids:endDate a ids:KindSemantics;
    rdfs:comment "end date";
    dct:identifier "END_DATE" ;
    skos:broader ids:date.

ids:textual a ids:KindSemantics;
    rdfs:comment "textual";
    dct:identifier "TEXTUAL";
    skos:narrower ids:freetext.

ids:binary a ids:KindSemantics;
    rdfs:comment "binary";
    dct:identifier "BINARY".

ids:image a ids:KindSemantics;
    rdfs:comment "image";
    dct:identifier "IMAGE".

ids:identifier a ids:KindSemantics;
    rdfs:comment "identifier";
    dct:identifier "IDENTIFIER".

ids:freetext a ids:KindSemantics;
    rdfs:comment "freetext";
    dct:identifier "FREETEXT";
    skos:broader ids:textual.

ids:sensor a ids:KindSemantics;
    rdfs:comment "sensor measurement";
    dct:identifier "SENSOR_MEASUREMENT".

