@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

@prefix ids_da: <https://schema.industrialdataspace.org/dataAsset/> .
@prefix ids_trans: <https://schema.iais.fraunhofer.de/ids/helper/transformation/> .

# Classes
# -------

ids_da:KindSemantics a owl:Class;
    ids_trans:isAbstract true;
    rdfs:label "KindSemantics"@en ;
    rdfs:comment "The class of defined high-level semantics for DataAsset Kindsorder policies of Collections.".

ids_da:CoreKindSemantics rdfs:subClassOf ids_da:KindSemantics;
    a owl:Class;
    rdfs:label "CoreKindSemantics"@en ;
    rdfs:comment "The class of KindSemantics defined in the core information model.".

# Instances
# ---------

ids_da:hash a ids_da:CoreKindSemantics;
    rdfs:comment "hash";
    dct:identifier "HASH".

ids_da:date a ids_da:CoreKindSemantics;
    rdfs:comment "date";
    dct:identifier "DATE";
    skos:narrower ids_da:startDate;
    skos:narrower ids_da:endDate.

ids_da:startDate a ids_da:CoreKindSemantics;
    rdfs:comment "start date";
    dct:identifier "START_DATE";
    skos:broader ids_da:date.

ids_da:endDate a ids_da:CoreKindSemantics;
    rdfs:comment "end date";
    dct:identifier "END_DATE" ;
    skos:broader ids_da:date.

ids_da:textual a ids_da:CoreKindSemantics;
    rdfs:comment "textual";
    dct:identifier "TEXTUAL".

ids_da:binary a ids_da:CoreKindSemantics;
    rdfs:comment "binary";
    dct:identifier "BINARY".

ids_da:image a ids_da:CoreKindSemantics;
    rdfs:comment "image";
    dct:identifier "IMAGE".

ids_da:identifier a ids_da:CoreKindSemantics;
    rdfs:comment "identifier";
    dct:identifier "IDENTIFIER".

ids_da:sensor a ids_da:CoreKindSemantics;
    rdfs:comment "sensor measurement";
    dct:identifier "SENSOR_MEASUREMENT".

