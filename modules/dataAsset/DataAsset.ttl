@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .

@prefix ids_da: <https://schema.industrialdataspace.org/dataAsset/> .
@prefix ids_base: <https://schema.industrialdataspace.org/base/> .
@prefix ids_rel: <https://schema.iais.fraunhofer.de/ids/helper/relation/> .
@prefix ids_conn: <https://schema.industrialdataspace.org/connector/> .

# Classes
# -------

ids_da:DataAsset rdfs:subClassOf ids_base:EntityWithLifecycle;
    rdfs:subClassOf ids_comm:Resource;
    rdfs:subClassOf dcat:Dataset;
    a owl:Class;
    rdfs:label "DataAsset"@en;
    rdfs:comment "Class describing data on the Industrial Dataspace. May be a data offering, data that is in the state of being transfered or an aggregation of multiple other dataassets."@en;
    ids_rel:validation [
        ids_rel:forProperty ids_da:coversTemporal;
        ids_rel:relationType ids_rel:OneToMany;
    ];
    ids_rel:validation [
        ids_rel:forProperty ids_da:coversSpatial;
        ids_rel:relationType ids_rel:OneToMany;
    ];
    ids_rel:validation [
        ids_rel:forProperty ids_da:coversCategory;
        ids_rel:relationType ids_rel:OneToMany;
    ];
    ids_rel:validation [
        ids_rel:forProperty ids_da:representation;
        ids_rel:relationType ids_rel:OneToMany;
    ];
    ids_rel:validation [
        ids_rel:forProperty ids_da:coversLanguage;
        ids_rel:relationType ids_rel:OneToMany;
    ].

# Properties
# ----------

ids_da:representation a owl:ObjectProperty;
    rdfs:label "representation"@en;
    rdfs:domain ids_da:DataAsset;
    rdfs:range ids_content:Representation;
    rdfs:comment "Representation of the DataAsset."@en.

ids_da:version rdfs:subPropertyOf dct:hasVersion;
    a owl:DatatypeProperty;
    rdfs:label "version"@en;
    rdfs:domain ids_da:DataAsset;
    rdfs:range xsd:string;
    rdfs:comment "Version identifier of the InfrastructureComponent."@en.

ids_da:temporal a owl:ObjectProperty;
    rdfs:subPropertyOf dct:temporal;
    rdfs:label "temporal"@en;
    rdfs:domain ids_da:DataAsset;
    rdfs:range ids_temp:TemporalEntity;
    rdfs:comment "Temporal period or instance covered by the DataAsset."@en.

ids_da:spatial a owl:ObjectProperty;
    rdfs:subPropertyOf dct:spatial;
    rdfs:label "spatial"@en;
    rdfs:domain ids_da:DataAsset;
    rdfs:range ids_spatial:SpatialEntity;
    rdfs:comment "Spatial location covered by the DataAsset."@en.

ids_da:coversLanguage a owl:ObjectProperty;
    rdfs:subPropertyOf dct:language;
    rdfs:label "coversLanguage"@en;
    ids_doc:labelPluralForm "coversLanguages"@en;
    rdfs:domain ids_da:DataAsset;
    rdfs:range ids_lang:Language;
    ids_trans:refByURI true;
    rdfs:comment "States the languages that are used by the content of the DataAsset."@en.

ids_da:origin a owl:DatatypeProperty;
    rdfs:label "origin"@en;
    rdfs:domain ids_da:DataAsset;
    rdfs:range xsd:anyURI;
    rdfs:comment "Link to the original resource from which the content of the DataAsset has been derived or is a copy of."@en.