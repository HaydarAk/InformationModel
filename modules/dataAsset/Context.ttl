@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dct: <http://purl.org/dc/terms/> .

@prefix ids_da: <https://schema.industrialdataspace.org/dataAsset/> .
@prefix ids_temp: <https://schema.industrialdataspace.org/temporal/> .
@prefix ids_spatial: <https://schema.industrialdataspace.org/spatialEntity/> .

# Classes
# -------

ids_da:Context a owl:Class;
    rdfs:label "Context"@en;
    rdfs:comment "Layered description of resource's reference in time (May 2018) , space (bounding box) and world (Building 101 of MyCompany)"@en.

# Properties
# ----------

ids_da:temporal a owl:ObjectProperty;
    rdfs:subPropertyOf dct:temporal;
    rdfs:label "temporal"@en;
    rdfs:domain ids_da:Context;
    rdfs:range ids_temp:TemporalEntity;
    rdfs:comment "Temporal period or instance defining a Context."@en.

ids_da:spatial a owl:ObjectProperty;
    rdfs:subPropertyOf dct:spatial;
    rdfs:label "spatial"@en;
    rdfs:domain ids_da:Context;
    rdfs:range ids_spatial:SpatialEntity;
    rdfs:comment "Spatial location defining a Context."@en.