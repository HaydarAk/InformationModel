@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dct: <http://purl.org/dc/terms/> .

@prefix ids_rel: <https://schema.iais.fraunhofer.de/ids/helper/relation/> .
@prefix ids_trans: <https://schema.iais.fraunhofer.de/ids/helper/transformation/> .
@prefix ids_doc: <https://schema.iais.fraunhofer.de/ids/helper/documentation/> .

# Classes
# -------

ids_comm:Communication a owl:Class;
    rdfs:label "Communication"@en;
    rdfs:comment "Means to communicate (receive / provide) resources."@en;
    ids_rel:validation [
        ids_rel:forProperty ids_comm:interface;
        ids_rel:relationType ids_rel:OneToMany;
    ].

ids_comm:DataService a owl:Class;
    rdfs:label "DataService"@en;
    rdfs:comment "Class of Services to invoke an Interface."@en.

# Properties
# ----------

ids_comm:interface a owl:ObjectProperty;
    rdfs:domain ids_comm:Communication;
    rdfs:range ids_comm:Interface;
    rdfs:label "interface"@en;
    ids_doc:labelPluralForm "interfaces"@en;
    rdfs:comment "Declares Interfaces to access a DataAsset."@en.




ids_comm:apiDocument
    range: string

ids_comm:apiDocumentReference
    range: URL

