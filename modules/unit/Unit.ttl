@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dct: <http://purl.org/dc/terms/> .

@prefix ids: <https://schema.industrialdataspace.org/> .
@prefix ids_unit: <https://schema.industrialdataspace.org/extcore/unit/> .

# Classes
# -------

ids:Unit a owl:Class;
    rdfs:label "Unit"@en ;
    rdfs:comment "The class of units.".

# Instances
# ---------

ids_unit:euro a ids:Unit;
    rdfs:comment "Euro";
    dct:identifier "EUR".

ids_unit:usd a ids:Unit;
    rdfs:comment "US Dollar";
    dct:identifier "USD".

ids_unit:kb a ids:Unit;
    rdfs:comment "Kilobyte";
    dct:identifier "KB".

