@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix ids: <https://w3id.org/ids/core> .

# Classes
# -------

ids:Unit a owl:Class;
    rdfs:label "Unit"@en ;
    rdfs:comment "The class of units.".

# Instances
# ---------

ids:euro a ids:Unit;
    rdfs:comment "Euro";
    dct:identifier "EUR".

ids:usd a ids:Unit;
    rdfs:comment "US Dollar";
    dct:identifier "USD".

ids:kb a ids:Unit;
    rdfs:comment "Kilobyte";
    dct:identifier "KB".

ids:day a ids:Unit;
    rdfs:comment "Day";
    dct:identifier "DAY".

ids:week a ids:Unit;
    rdfs:comment "Week";
    dct:identifier "WEEK".

ids:month a ids:Unit;
    rdfs:comment "Month";
    dct:identifier "MONTH".

ids:year a ids:Unit;
    rdfs:comment "Year";
    dct:identifier "YEAR".