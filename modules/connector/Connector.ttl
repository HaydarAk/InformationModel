@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix ids: <https://w3id.org/ids/core> .

# Classes
# -------

ids:Connector rdfs:subClassOf ids:InfrastructureComponent;
    rdfs:subClassOf dcat:Catalog;
    a owl:Class;
    rdfs:label "Connector"@en ;
    rdfs:comment "Connector for hosting data services."@en;
    ids:validation [
        ids:forProperty ids:provides;
        ids:relationType ids:OneToMany;
    ].

ids:Resource a owl:Class;
    ids:isAbstract true;
    rdfs:label "Resource"@en ;
    rdfs:comment "Superclass of resources that can be provided by a Connector."@en.

ids:AuthInfo a owl:Class;
    rdfs:label "AuthInfo"@en ;
    rdfs:comment "Holds authentication information so that connections to the Connector's data offerings can be establised."@en;
    ids:validation [
        ids:forProperty ids:authService;
        ids:constraint ids:NotNull;
    ].

# Properties
# ----------

ids:provides a owl:ObjectProperty;
    rdfs:domain ids:Connector;
    rdfs:range ids:Resource;
    rdfs:label "provides"@en;
    rdfs:comment "The Resources provided by a Connector."@en.

ids:securityProfile a owl:ObjectProperty;
    rdfs:domain ids:Connector;
    rdfs:range ids:SecurityProfile;
    rdfs:label "securityProfile"@en;
    rdfs:comment "The SecurityProfile supported by the Connector."@en.

ids:authInfo a owl:ObjectProperty;
    rdfs:domain ids:Connector;
    rdfs:range ids:AuthInfo;
    rdfs:label "authInfo"@en;
    rdfs:comment "Information of the authentication service used by the Connector."@en.

ids:authService a owl:ObjectProperty;
    rdfs:domain ids:AuthInfo;
    rdfs:range xsd:anyURI;
    rdfs:label "authService"@en;
    rdfs:comment "The URL of the authentication service that is provided by the Connector."@en.

ids:authStandard a owl:ObjectProperty;
    rdfs:domain ids:AuthInfo;
    rdfs:range ids:AuthStandard;
    rdfs:label "authStandard"@en;
    rdfs:comment "The standard used by the authentication service that is provided by the Connector."@en.